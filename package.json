{
  "name": "bonsai-front-node",
  "version": "1.0.0",
  "description": "Full-stack React and Node.js application",
  "private": true,
  "scripts": {
    "start": "node server/index.js",
    "server": "cd server && pnpm run dev",
    "client": "cd client && pnpm run dev",
    "dev": "concurrently \"pnpm run server\" \"pnpm run client\"",
    "build": "cd client && pnpm run build",
    "preview": "cd client && pnpm run preview"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 